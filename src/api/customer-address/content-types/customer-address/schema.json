{
  "kind": "collectionType",
  "collectionName": "customer_addresses",
  "info": {
    "singularName": "customer-address",
    "pluralName": "customer-addresses",
    "displayName": "Customer Address"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "addressType": {
      "type": "enumeration",
      "enum": [
        "shipping",
        "billing"
      ]
    },
    "defaultAddress": {
      "type": "boolean"
    },
    "customerId": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::customer.customer",
      "inversedBy": "customerAddresses"
    }
  }
}
